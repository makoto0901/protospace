- if user_signed_in?
  - if prototype.like_user(current_user.id)
    = button_to prototype_like_path(id: like.id, prototype_id: prototype.id), method: :delete, class: "btn btn-default", remote: true do
      = image_tag("icon_heart_red.svg")
      LIKE
      %span.num
        = @prototype.likes_count
  - else
    = button_to prototype_likes_path(prototype), class: "btn btn-default",id: "button", remote: true do
      = image_tag("icon_heart.svg")
      LIKE
      %span.num
        = @prototype.likes_count
- else
  = button_to prototype_like_path(prototype_id: prototype.id), method: :delete, class: "btn btn-default", remote: true do
    = image_tag("icon_heart_red.svg")
    LIKE
    %span.num
      = @prototype.likes_count
